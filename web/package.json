{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:graphql": "graphql-codegen --config graphql-codegen.config.js",
    "build:icons:solid": "svgr node_modules/boxicons/svg/solid --out-dir ./generated/icons/solid --config-file svgr.config.js",
    "build:icons:logos": "svgr node_modules/boxicons/svg/logos --out-dir ./generated/icons/logos --config-file svgr.config.js",
    "postinstall": "npm run build:graphql && npm run build:icons:solid && npm run build:icons:logos",
    "introspect": "npx env-cmd graphql-codegen --config graphql-introspect.config.js",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.0",
    "@mantine/hooks": "^4.2.1",
    "@tiptap/core": "^2.0.0-beta.175",
    "@tiptap/extension-character-count": "^2.0.0-beta.26",
    "@tiptap/extension-link": "^2.0.0-beta.37",
    "@tiptap/extension-placeholder": "^2.0.0-beta.48",
    "@tiptap/react": "^2.0.0-beta.109",
    "@tiptap/starter-kit": "^2.0.0-beta.184",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-graphcache": "^4.3.6",
    "aws-sdk": "^2.1126.0",
    "boxicons": "^2.1.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "firebase": "^9.6.10",
    "firebase-admin": "^10.0.2",
    "graphql": "^16.3.0",
    "install": "^0.13.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nanoid": "^3.3.3",
    "next": "12.1.5",
    "next-connect": "^0.12.2",
    "npm": "^8.10.0",
    "react": "18.0.0",
    "react-avatar-editor": "^13.0.0",
    "react-dom": "18.0.0",
    "react-dropzone": "^14.2.1",
    "react-hot-toast": "^2.2.0",
    "react-textarea-autosize": "^8.3.3",
    "recoil": "^0.7.2",
    "urql": "^2.2.0",
    "uuid": "^8.3.2",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
